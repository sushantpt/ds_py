   "source": [
    "# finding factorial no using loops. Time Complexity =  O(n)\n",
    "def factorial(n):\n",
    "    start = 1\n",
    "    for i in range(n, 0, -1):\n",
    "        start = start * i\n",
    "    return start\n",
    "asking_user = int(input(\"Enter a number: \"))\n",
    "print(factorial(asking_user))"
   ]
