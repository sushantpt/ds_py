   "source": [
    "# recurssive way to find factorial no. Time complexity= O(n)\n",
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        factor = n * factorial(n-1)\n",
    "        return factor\n",
    "user_input = (int(input(\"Enter number:\")))\n",
    "    \n",
    "factorial(user_input)"
   ]
  }
